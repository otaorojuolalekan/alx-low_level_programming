#include <stdio.h>

/**
 * main - Entry point
 *
 * Return: Always 0 (Success)
 */l
int main(void)
{
	printf("Size of a char: %d\n", sizeof(char));
	printf("Size of an int: %d\n", sizeof(int));
	printf("Size of a long int: %d\n", sizeof(long int));
	printf("Sieze of a long long int", sizeof(long long int))
		)
	return (0);
}
